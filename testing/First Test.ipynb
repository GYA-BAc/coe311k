    "def a_pi(n):\n",
    "    inside = 0\n",
    "    for _ in range(0, n):\n",
    "        inside += int((random.random()**2 + random.random()**2)**(1/2) < 1)\n",
    "            \n",
    "    return (4*inside/n)"
   ]
